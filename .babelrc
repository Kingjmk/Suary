{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    ["module-resolver", {
      "root": "./frontend/src",
      "alias": {
        "~": "./frontend/src",
        "test": "./frontend/src",
        "underscore": "lodash"
      }
    }]
  ]
}
